{
  "name": "Simple Profanity Filter",
  "version": "1.2.1",
    "manifest_version" : 2,
  "description": "A simple and configurable profanity filter.",
  "icons": { "128": "icon.png" },
  "background": {
    "scripts" : ["background.js"]
  },
  "options_page": "options.html",
  "content_scripts": [
  {
    "matches": ["*://*/*"],
    "js": ["filter.js"],
    "run_at": "document_start",
    "all_frames": true
    }
  ]
}
